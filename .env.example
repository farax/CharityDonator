# Aafiyaa Charity Clinics - Environment Variables Example
# Copy this file to .env and fill in the actual values

# App Settings
NODE_ENV=development
PORT=5000
SESSION_SECRET=your_session_secret

# New Relic Monitoring (optional)
NEW_RELIC_LICENSE_KEY=
VITE_NEW_RELIC_ACCOUNT_ID=
VITE_NEW_RELIC_APPLICATION_ID=
VITE_NEW_RELIC_BROWSER_LICENSE_KEY=

# Stripe Payment Gateway
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
VITE_STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key
STRIPE_WEBHOOK_SECRET=whsec_your_stripe_webhook_secret

# PayPal Payment Gateway
PAYPAL_CLIENT_ID=your_paypal_client_id
PAYPAL_SECRET_KEY=your_paypal_secret_key
VITE_PAYPAL_CLIENT_ID=your_paypal_client_id

# Email Service (SendGrid)
SENDGRID_API_KEY=your_sendgrid_api_key

# Database Configuration
DATABASE_URL=postgres://username:password@localhost:5432/aafiyaa
PGUSER=username
PGPASSWORD=password
PGDATABASE=aafiyaa
PGHOST=localhost
PGPORT=5432